// 字串操作範例
fn count_char(s: String, c: String) -> Int {
    let count = 0;
    let i = 0;
    while i < len(s) {
        if s[i] == c[0] {
            count = count + 1;
        }
        i = i + 1;
    }
    return count;
}

fn main() {
    let text = "Hello, UAD!";
    
    let count_l = count_char(text, "l");
    
    println("Original: " + text);
    println("Count of 'l': " + count_l);
}

