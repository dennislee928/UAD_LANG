fn main() -> Int {
    println("Test: math operations");
    
    let a = 5;
    let b = 3;
    
    let sum = a + b;
    let diff = a - b;
    let prod = a * b;
    let quot = a / b;
    
    if sum == 8 && diff == 2 && prod == 15 && quot == 1 {
        println("✓ Pass: basic arithmetic works");
    } else {
        println("✗ Fail: arithmetic error");
        return 1;
    }
    
    return 0;
}

